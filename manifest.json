{
  "name": "Quora Enhancer",
  "version": "1.5",
  "manifest_version": 2,
  "description": "A Chrome Extension to enhance the Quora Experience",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "page": "background.html"
  },
  "omnibox":{
    "keyword": "quora"
  },
  "browser_action": {

    "default_popup": "popup.html"
  },
  "icons":{
      "48":"icons/icon-48.png","128":"icons/icon-128.png"
    },
  "permissions": ["<all_urls>", "notifications", "storage","tabs","contextMenus"]
}
